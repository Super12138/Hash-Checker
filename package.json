{
    "name": "super-hash",
    "version": "0.0.0",
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build:web": "run-p type-check \"build-only:web {@}\" --",
        "build:desktop": "run-p type-check \"build-only:desktop {@}\" --",
        "build:store": "run-p type-check \"build-only:store {@}\" --",
        "build-only:web": "vite build --mode web",
        "build-only:desktop": "vite build --mode desktop",
        "build-only:store": "vite build --mode store",
        "type-check": "vue-tsc --build",
        "preview": "vite preview",
        "format": "prettier --write src/"
    },
    "dependencies": {
        "@mdui/icons": "^1.0.3",
        "@vueuse/core": "^13.6.0",
        "crypto-js": "^4.2.0",
        "mdui": "^2.1.4",
        "pinia": "^3.0.3",
        "pinia-plugin-persistedstate": "^4.4.1",
        "vue": "^3.5.18",
        "vue-i18n": "^11.1.11"
    },
    "devDependencies": {
        "@tsconfig/node24": "^24.0.1",
        "@types/crypto-js": "^4.2.2",
        "@types/node": "^24.0.14",
        "@vite-pwa/assets-generator": "^1.0.0",
        "@vitejs/plugin-vue": "^6.0.0",
        "@vue/tsconfig": "^0.7.0",
        "npm-run-all2": "^8.0.4",
        "prettier": "3.6.2",
        "typescript": "^5.9.2",
        "vite": "^7.1.1",
        "vite-plugin-html": "^3.2.2",
        "vite-plugin-pwa": "^1.0.2",
        "vite-plugin-vue-devtools": "^8.0.0",
        "vue-tsc": "^3.0.5",
        "workbox-core": "^7.3.0",
        "workbox-precaching": "^7.3.0",
        "workbox-routing": "^7.3.0",
        "workbox-strategies": "^7.3.0",
        "workbox-window": "^7.3.0"
    }
}
